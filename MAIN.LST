				    PUSH BX
				    PUSH CX
				    PUSH DX
				    PUSH BP
				        ;DIVIDE THE FIRST SIZE BY 2
				        MOV CX, SIZE_1
				        SHR CX, 1
				        ;DIVIDE THE SECOND SIZE BY 2
				        MOV DX, SIZE_2
				        SHR DX, 1
				        ;GET THE SUPPOSED DISTANCE BETWEEN THEI
				R CENTERS
				        MOV BP , 0
				        ADD BP, DX
				        ADD BP, CX
				        ;INORDER TO MAKE SURE THE TWO SQUARES A
				RE COLLIDING NOT ONLY PASSING BY
				        SUB BP ,3 
				        ;GET THE ACTUAL DISTANCE BETWEEN THEIR 
				CENTERS
				        ;GET THE CENTER X COORDINATE OF BOTH
				        MOV AX, STARTPOS_1X
				        ADD AX, CX
				        MOV BX, STARTPOS_2X
				        ADD BX, DX
				        ;SUBTRACT THE TWO X COORDINATES TO GET 
				THE ACTUAL ABSOLUTE DISTANCE BETWEEN THEIR CENT
				ERS IN X
				        CMP AX,BX
				        JL COMMUTATEX
